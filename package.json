    "d3-scale": "^4.0.2",
    "default-passive-events": "^2.0.0",
    "ejs": "^3.1.6",
    "element-plus": "^2.2.18",
    "express": "4.18.1",
    "express-async-errors": "^3.1.1",
    "html2canvas": "^1.0.0-rc.5",
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "nedb-promises": "^5.0.2",
    "qrcode": "^1.5.0",
    "uuid": "^8.3.2",
    "vant": "3.4.6",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
