{
  "name": "h5editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "back-server": "cross-env NODE_ENV=development nodemon -e ts -w ./server -x   ts-node-transpile-only ./server/src/index.ts",
    "front-serve": "vue-cli-service serve",
    "build-previewer": "vue-cli-service build --mode lib --target lib --name index ./src/components/Previewer/index.ts",
    "build-front": "vue-cli-service build",
    "build-server": "npm run build-previewer && tsc --build ./server/tsconfig.json && gulp",
    "build": "npm run build-front && npm run build-server",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.4",
